{
  "name": "front",
  "description": "Front end for mxsh.io",
  "repository": "https://github.com/kindnessorg/kindness-frontend",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "if [ \"$NODE_ENV\" = \"production\" ]; then yarn prod; else yarn dev; fi",
    "prod": "yarn server",
    "dev": "concurrently 'yarn compile:client:dev' 'yarn compile:server:dev' 'yarn server'",
    "compile": "yarn compile:client && yarn compile:server",
    "compile:client": "node_modules/.bin/webpack --config ./webpack.config.client.js",
    "compile:server": "node_modules/.bin/webpack --config ./webpack.config.server.js",
    "compile:client:dev": "node_modules/.bin/webpack --config ./webpack.config.client.js --watch",
    "compile:server:dev": "node_modules/.bin/webpack --config ./webpack.config.server.js --watch",
    "lint": "node_modules/.bin/eslint src",
    "test": "node_modules/.bin/mocha --require test/setup.js --recursive --compilers js:babel-register,js:babel-polyfill",
    "server": "node ./dist/server.js"
  },
  "dependencies": {
    "babel-core": "^6.22.1",
    "babel-loader": "^6.2.10",
    "babel-polyfill": "^6.22.0",
    "babel-register": "^6.22.0",
    "cookie-parser": "^1.4.3",
    "email-addresses": "^2.0.2",
    "express": "^4.14.0",
    "fs": "^0.0.1-security",
    "history": "^3.2.0",
    "http-proxy-middleware": "^0.17.3",
    "isomorphic-fetch": "^2.2.1",
    "js-cookie": "^2.1.3",
    "lodash": "^4.17.4",
    "path-browserify": "^0.0.0",
    "pg": "^6.1.2",
    "raven": "^1.1.1",
    "raven-js": "^3.10.0",
    "react": "^15.4.2",
    "react-cookie": "^1.0.4",
    "react-copy-to-clipboard": "^4.2.3",
    "react-dom": "^15.4.2",
    "react-redux": "^5.0.2",
    "react-router": "^3.0.2",
    "react-router-redux": "^4.0.7",
    "redial": "^0.5.0",
    "redux": "^3.6.0",
    "redux-actions": "^1.2.1",
    "redux-catch": "^1.3.1",
    "redux-devtools-extension": "^1.0.0",
    "redux-form": "^6.5.0",
    "redux-saga": "^0.14.3",
    "validator": "^6.2.1"
  },
  "devDependencies": {
    "babel-eslint": "^7.1.1",
    "babel-preset-latest": "^6.22.0",
    "babel-preset-react": "^6.22.0",
    "babel-preset-stage-0": "^6.22.0",
    "chai": "^3.5.0",
    "chokidar": "^1.6.1",
    "concurrently": "^3.1.0",
    "copy-webpack-plugin": "^4.0.1",
    "eslint": "^3.14.1",
    "eslint-config-airbnb": "^14.0.0",
    "eslint-loader": "^1.6.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^3.0.2",
    "eslint-plugin-react": "^6.9.0",
    "mocha": "^3.2.0",
    "nodemon": "^1.11.0",
    "react-hot-loader": "^3.0.0-beta.6",
    "webpack": "^2.2.1",
    "webpack-dev-middleware": "^1.9.0",
    "webpack-dev-server": "^1.16.2",
    "webpack-hot-middleware": "^2.15.0",
    "webpack-node-externals": "^1.5.4"
  }
}
