

{
  "masterPicture": "./src/icons/shield-260x260.png",
  "iconsPath": "/",
  "design": {
    "ios": {
      "pictureAspect": "backgroundAndMargin",
      "margin": "25%",
      "assets": {
        "ios6AndPriorIcons": true,
        "ios7AndLaterIcons": true,
        "precomposedIcons": true,
        "declareOnlyDefaultIcon": true
      }
    },
    "desktopBrowser": {},
    "windows": {
      "pictureAspect": "whiteSilhouette",
      "backgroundColor": "#00aba9",
      "onConflict": "override",
      "assets": {
        "windows80Ie10Tile": false,
        "windows10Ie11EdgeTiles": {
          "small": false,
          "medium": true,
          "big": false,
          "rectangle": false
        }
      }
    },
    "androidChrome": {
      "pictureAspect": "shadow",
      "themeColor": "#ffffff",
      "manifest": {
        "name": "Mail Shield",
        "display": "standalone",
        "orientation": "notSet",
        "onConflict": "override",
        "declared": true
      },
      "assets": {
        "legacyIcon": false,
        "lowResolutionIcons": false
      }
    },
    "safariPinnedTab": {
      "pictureAspect": "silhouette",
      "themeColor": "#5bbad5"
    }
  },
  "settings": {
    "scalingAlgorithm": "Mitchell",
    "errorOnImageTooSmall": false
  }
}

